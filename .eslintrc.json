{
  "env": {
    "browser": true,
    "qunit": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",
      4
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-console": 0,
    "no-unused-vars": 0
  },
  "parserOptions": {
    "ecmaVersion": 6
  }
}
